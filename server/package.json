{
  "author": "Robert Ostermann",
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "build/server.js",
  "scripts": {
    "build": "npm run esbuild",
    "start": "node build/server.js",
    "dev": "concurrently \"npm run esbuild-watch\" \"nodemon --inspect=0.0.0.0:9229 -q build/server.js\"",
    "esbuild": "rimraf build && esbuild ./src/server.ts --outfile=build/server.js --bundle --minify --platform=node --external:'pg-native' --external:'superagent-proxy'",
    "esbuild-watch": "esbuild ./src/server.ts --outfile=build/server.js --watch --bundle --sourcemap --platform=node --external:'pg-native' --external:'superagent-proxy'"
  },
  "dependencies": {
    "@types/auth0": "^2.35.7",
    "@types/express": "^4.17.14",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "auth0": "^2.44.0",
    "axios": "^1.2.0",
    "camelcase-keys": "^8.0.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.16",
    "eslint": "^8.28.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "express-jwt-authz": "^2.4.1",
    "json2csv": "^5.0.7",
    "jwks-rsa": "^3.0.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13"
  }
}
