{
  "author": "Robert Ostermann",
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "build": "npm run esbuild",
    "start": "node build/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon --inspect=0.0.0.0:9229 -q build/index.js\"",
    "esbuild": "rimraf build && esbuild ./src/index.ts --outfile=build/index.js --bundle --minify --platform=node --external:'pg-native' --external:'superagent-proxy'"
  },
  "dependencies": {
    "auth0": "^2.40.0",
    "axios": "^0.26.1",
    "camelcase-keys": "^7.0.2",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "esbuild": "^0.15.7",
    "eslint": "^8.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.0.5",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/auth0": "^2.34.17",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0"
  }
}
